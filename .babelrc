{
  "presets": [
    ["@babel/preset-env", { "modules": false }]
  ],
  "plugins": ["@babel/plugin-transform-flow-strip-types"],
  "env": {
    "cjs": {
      "presets": [
        ["@babel/preset-env", { "modules": "commonjs" }]
      ]
    },
    "coverage": {
      "plugins": [
        ["babel-plugin-istanbul", { "include": ["src/**/*.js"] }]
      ]
    }
  }
}
